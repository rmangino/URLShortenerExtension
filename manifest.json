{
  "name": "goo.gl url shortener",
  "description": "Shorten the url of the current tab",
  "version": "1.0",
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "browser_action": {
    "default_title": "goo.gl url shortener",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}